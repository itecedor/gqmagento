<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
<acl>
    <resources>
        <resource id="Magento_Backend::admin">
			<!-- Menu of Extension -->
			<resource id="Milople_All::all" title="Milople Extensions" sortOrder="10" >
				<resource id="Milople_Requestforquote::requestforquote" title="Milople Request for Quote" sortOrder="40">
					<resource id="Milople_Requestforquote::enquiries" title="Enquiries" sortOrder="40">
					</resource>
					<resource id="Milople_Requestforquote::config" title="Configurations" sortOrder="50">
					</resource>
				</resource>
			</resource>
            <!-- System Config Section of Extension -->
            <resource id="Magento_Backend::stores">
                <resource id="Magento_Backend::stores_settings">
                    <resource id="Magento_Config::config">
                        <resource id="Milople_Requestforquote::config_requestforquote" title="Milople Request For System Config Section" />
                    </resource>
                </resource>
            </resource>
        </resource>
    </resources>
</acl>
</config>