<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation=".urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Lof_Affiliate::affiliate_elements" title="Lof Affiliate">
                    <resource id="Lof_AffiliateSaveCart::cart" title="Manage Cart" sortOrder="100">
                        <resource id="Lof_AffiliateSaveCart::cart_new" title="New Cart" sortOrder="5" />
                        <resource id="Lof_AffiliateSaveCart::cart_edit" title="Edit Cart" sortOrder="10" />
                        <resource id="Lof_AffiliateSaveCart::cart_delete" title="Delete Cart" sortOrder="20" />
                        <resource id="Lof_AffiliateSaveCart::cart_save" title="Save Cart" sortOrder="30" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>